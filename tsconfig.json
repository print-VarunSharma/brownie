{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        // "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "skipLibCheck": true,
        "incremental": true
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "components/Cards/CreatorCard/CreatorImageCard.jsx",
        "components/Auth/authForm.jsx",
        "components/Auth/Logo.jsx",
        "components/Auth/OAuthButtonGroup.jsx",
        "components/Auth/PasswordField.jsx"
    ],
    "exclude": ["node_modules"]
}
